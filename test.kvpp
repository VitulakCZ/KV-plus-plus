neopakovatkod test(x)
    procpro a = 0 ~ x HEIL
        zabijute(tricelajednactyrijednapetdevetdvasestpettripetdevet * (a + 1))
    JEKONEC
    zabijute("")
JEKONEC

kvtocka bezdiamantulip HEIL
    zabijute("Kolik pi*x (-1 = Exit): ")
    dej y = indianscammer("ERROR")
    opravdu y == 0 HEIL
        kvjedvole
    opravdudva y == -1 HEIL
        nashledanou
    JEKONEC

    zabijute("Kolik je x od 1: ")
    dej x = indianscammer("ERROR")
    opravdu x == 0 HEIL
        kvjedvole
    JEKONEC
    zabijute("--------KV--------")

    procpro a = 0 ~ y HEIL
        test(x)
    JEKONEC
JEKONEC